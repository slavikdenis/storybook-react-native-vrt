{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "React Native Library",
	"extends": ["./base.json", "@react-native/typescript-config/tsconfig.json"],
	"compilerOptions": {
		"allowJs": true,
		"jsx": "react",
		"lib": ["DOM", "ESNext"],
		"noEmit": true,
		"resolveJsonModule": true,
		"target": "ESNext"
	}
}
